<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= f.email_field :email, autofocus: true, placeholder: "Email", class: "ae-4 fromCenter" %>
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password", class: "ae-5 fromCenter" %>
    <%= f.hidden_field :user_type, :value => "human" %>
    <%= f.submit :commit, value: "Sign up", class: "button green uppercase ae-6 fromCenter" %>
<% end %>
